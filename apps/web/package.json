{
  "name": "@parking-ticket-pal/web",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "turbo db:generate && next dev",
    "build": "next build",
    "start": "next start",
    "email": "email dev --port 3005",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "next test",
    "test:watch": "jest --watch",
    "check-types": "tsc",
    "prettier": "prettier . --ignore-path .gitignore '!pnpm-lock.yaml' '**/*.+(js|mjs|jsx|json|ts|tsx|css|html|md)'",
    "format": "pnpm prettier --write",
    "check-format": "pnpm prettier --list-different",
    "preinstall": "pnpm dlx only-allow pnpm",
    "db:generate": "cd ../../packages/db && pnpm db:generate",
    "db:studio": "cd ../../packages/db && pnpm db:studio",
    "db:migrate": "cd ../../packages/db && pnpm db:migrate",
    "db:deploy": "cd ../../packages/db && pnpm db:deploy",
    "db:push": "cd ../../packages/db && pnpm db:push",
    "db:seed": "cd ../../packages/db && pnpm db:seed",
    "prepare": "husky",
    "scrape:contravention-codes": "tsx utils/scraping/getContraventionCodes.ts",
    "generate:te9": "tsx utils/automation/forms/TE9.ts",
    "generate:te7": "tsx utils/automation/forms/TE7.ts",
    "generate:pe2": "tsx utils/automation/forms/PE2.ts",
    "generate:pe3": "tsx utils/automation/forms/PE3.ts",
    "generate:blog": "tsx scripts/generate-blog.ts",
    "generate:blog-manual": "tsx scripts/generate-blog-manual.ts",
    "test:social": "tsx scripts/test-social-posting.ts",
    "check:tokens": "tsx scripts/check-social-tokens.ts",
    "setup:tokens": "tsx scripts/setup-social-tokens.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit ./next-env.d.ts ./next-auth.d.ts ./oslllo-potrace.d.ts",
      "eslint --fix"
    ],
    "**/*.+(js|jsx|json|ts|tsx|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "jest --findRelatedTests --passWithNoTests"
    ],
    "../../packages/db/prisma/schema.prisma": [
      "pnpm --filter @parking-ticket-pal/db prisma format"
    ]
  },
