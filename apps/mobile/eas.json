{
  "cli": {
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      },
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development"
      },
      "autoIncrement": false
    },
    "preview": {
      "extends": "base",
      "environment": "preview",
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "preview"
      },
      "autoIncrement": true
    },
    "production": {
      "extends": "base",
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "production"
      },
      "autoIncrement": true,
      "channel": "production"
    },
    "beta": {
      "extends": "production",
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "beta"
      },
      "autoIncrement": true,
      "channel": "beta"
    },
    "e2e-test": {
      "extends": "base",
      "withoutCredentials": true,
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_ENVIRONMENT": "development"
      },
      "autoIncrement": false
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "production"
      },
      "ios": {
        "appleId": "developer@chewybytes.com",
        "ascAppId": "6753653055",
        "appleTeamId": "HGX827L49J"
      }
    },
    "beta": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      },
      "ios": {
        "appleId": "developer@chewybytes.com",
        "ascAppId": "6753653055",
        "appleTeamId": "HGX827L49J"
      }
    }
  }
}