    "@fortawesome/react-native-fontawesome": "^0.3.2",
    "@hookform/resolvers": "^4.0.0",
    "@react-native-google-signin/google-signin": "^12.2.1",
    "@react-navigation/native": "^7.0.0",
    "@sentry/react-native": "^6.14.0",
