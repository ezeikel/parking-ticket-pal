    "@babel/core": "^7.26.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.14",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "mime-types": "^2.1.35",
    "react-test-renderer": "18.3.1",
