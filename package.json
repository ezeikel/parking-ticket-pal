{
  "name": "parking-ticket-pal",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "clean": "rm -rf apps/*/node_modules packages/*/node_modules",
    "clean:deep": "rm -rf apps/*/node_modules packages/*/node_modules pnpm-lock.yaml && pnpm install",
    "postinstall": "patch-package --patch-dir apps/mobile/patches"
  },
  "devDependencies": {
    "patch-package": "^8.0.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.6.1",
  "engines": {
    "node": ">=18"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-renderer": "19.1.0",
    "@types/react": "^19.0.14"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@shopify/react-native-skia"
    ]
  }
}
